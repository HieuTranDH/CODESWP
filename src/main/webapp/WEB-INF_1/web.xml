<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>Customer_login</servlet-name>
        <servlet-class>control.Customer_login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Customer_register</servlet-name>
        <servlet-class>control.Customer_register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Customer_logout</servlet-name>
        <servlet-class>control.Customer_logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Customer_profile</servlet-name>
        <servlet-class>control.Customer_profile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Customer_newpass</servlet-name>
        <servlet-class>control.Customer_newpass</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Customer_searchMovie</servlet-name>
        <servlet-class>control.Customer_searchMovie</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Admin_Promotion</servlet-name>
        <servlet-class>control.Admin_Promotion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Admin_ListCinema</servlet-name>
        <servlet-class>control.Admin_ListCinema</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Staff_screenroom</servlet-name>
        <servlet-class>control.Staff_screenroom</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Staff_seat</servlet-name>
        <servlet-class>control.Staff_seat</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Staff_combo</servlet-name>
        <servlet-class>control.Staff_combo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Customer_searchResult</servlet-name>
        <servlet-class>control.Customer_searchResult</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Admin_Analytics</servlet-name>
        <servlet-class>control.Admin_Analytics</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Customer_login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Customer_register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Customer_logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Customer_profile</servlet-name>
        <url-pattern>/profile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Customer_newpass</servlet-name>
        <url-pattern>/newpass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Customer_searchMovie</servlet-name>
        <url-pattern>/searchMovie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Admin_Promotion</servlet-name>
        <url-pattern>/staff/promotion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Admin_ListCinema</servlet-name>
        <url-pattern>/staff/listcinema</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Staff_screenroom</servlet-name>
        <url-pattern>/staff/screenroom</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Staff_seat</servlet-name>
        <url-pattern>/staff/seat</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Staff_combo</servlet-name>
        <url-pattern>/staff/combo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Customer_searchResult</servlet-name>
        <url-pattern>/searchresult</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Admin_Analytics</servlet-name>
        <url-pattern>/staff/adminanalytic</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>main</welcome-file>
    </welcome-file-list>
</web-app>
